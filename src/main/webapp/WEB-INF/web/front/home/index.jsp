                                            autoTab();
                                        })
                                    })
                                    $(".floor4-tab-list li .tab-list-content").each(function (item) {
                                        $(this).on("mouseover", function () {
                                            clearAutoTab();
                                        })
                                        $(this).on("mouseout", function () {
                                            autoTab();
                                        })
                                    })
                                }

                                //tab切换样式改变
